{
  "name": "@dracoui/primitives",
  "version": "0.1.14",
  "description": "Typography, spacing, and color primitives for Corinvo's Draco Design System",
  "keywords": [
    "corinvo",
    "draco",
    "dracoui",
    "design primitives",
    "primitives",
    "style dictionary"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "typecheck": "yarn tsc --noEmit",
    "lint": "yarn eslint --quiet --ext .js,.ts",
    "build": "npm run build:clean && npm run build:tokens",
    "build:clean": "rm -rf ./dist",
    "build:tokens": "tsx ./scripts/buildTokens.ts"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "colord": "^2.9.3",
    "eslint": "^8.57.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "lodash-es": "^4.17.21",
    "path": "^0.12.7",
    "prettier": "^3.4.2",
    "style-dictionary": "^4.3.0",
    "tinycolor2": "^1.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "geist": "^1.3.1",
    "jetbrains-mono": "^1.0.6"
  }
