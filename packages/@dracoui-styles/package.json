{
  "name": "@dracoui/styles",
  "version": "1.1.0",
  "type": "module",
  "description": "Global configurable styles for Corinvo's Draco Design System",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm exec npm-run-all --parallel build:*",
    "build:css": "sass index.scss dist/css/styles.css --no-source-map --load-path=node_modules && postcss dist/css/styles.css --replace --no-map",
    "build:css:unminified": "sass index.scss dist/css/styles.unminified.css --no-source-map --load-path=node_modules",
    "build:scss": "cpx 'src/**/*.scss' dist/scss && cpx 'index.scss' dist",
    "watch": "pnpm clean && pnpm exec npm-run-all --parallel watch:*",
    "watch:css": "sass index.scss dist/css/styles.css --watch --no-source-map --load-path=node_modules",
    "watch:scss": "cpx 'src/**/*.scss' dist/scss --watch"
  },
  "keywords": [
    "corinvo",
    "draco",
    "draco-design-system"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@dracoui/primitives": "0.2.37"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cpx2": "^8.0.0",
    "cssnano": "^7.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-normalize-charset": "^7.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
