<div class="draco-avatar-github" aria-label={{concat "Avatar for " this.username}} ...attributes>
  <Draco::Avatar @src={{this.userProfilePicture}} @size={{this.size}} @shape={{this.shape}} @alt={{if this.alt this.alt this.username}} />

  <Draco::Icon @name="mdi:github" @size={{28}} @color="white" class={{this.classNames}} />
</div>