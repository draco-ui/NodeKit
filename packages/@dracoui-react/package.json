{
  "name": "@dracoui/react",
  "version": "1.0.0",
  "description": "Draco Design System for React",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css",
    "./src/index.ts",
    "./dist/index.js",
    "./dist/index.cjs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && node -e \"const fs=require('fs');const src='dist/index.d.ts';const dest='dist/index.d.cts';fs.existsSync(src)&&fs.copyFileSync(src,dest);\"",
    "clean": "rimraf dist",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "corinvo",
    "draco",
    "design system",
    "react",
    "components",
    "ui"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@dracoui/styles": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@dracoui/types": "workspace:*",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@necto-react/hooks": "^2.12.5",
    "@necto/dom": "^1.4.5",
    "add": "^2.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ldrs": "^1.1.9",
    "react-aria-components": "^1.13.0",
    "react-if": "^4.1.6",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-measure": "^8.6.14",
    "@storybook/addon-outline": "^8.6.14",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "chromatic": "^13.3.2",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-hash-classname": "^0.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.93.3",
    "storybook": "^8.6.14",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.8"
  }
}
